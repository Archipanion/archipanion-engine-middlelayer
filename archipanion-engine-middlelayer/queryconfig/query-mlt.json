{
  "name": "Temporal Clip Query",
  "description": "This pipeline takes a number of input texts and query the retrievals for the given temporal sequence.",
  "inputs": {
    "input#": {"type": "TEXT"}
  },
  "operations": {
    "clip#" : {"type": "RETRIEVER", "name": "ClipRetriever", "input": "input#", "properties": {"field": "clip"}},
    "lookup#" : {"type": "TRANSFORMER", "name": "FieldLookup", "input": "clip#", "properties": {"field": "time", "keys": "start, end"}},
    "relations#" : {"type": "TRANSFORMER", "name": "RelationExpander", "input": "lookup#", "properties": {"outgoing": "partOf"}},
    "temporal" : {"type": "AGGREGATOR", "name": "TemporalSequenceAggregator", "input": "relations#"},
    "score" : {"type": "TRANSFORMER", "name": "ScoreAggregator",  "input": "temporal"},
    "lookup" : {"type": "TRANSFORMER", "name": "FieldLookup", "input": "score", "properties": {"field": "file", "keys": "path"}}
  },
  "output": "lookup"
}